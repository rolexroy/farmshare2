<%- include('partials/header') %>
    <h3>Registration</h3>
    <div class='conainer'>
        <div>
            <form action="/farmer/new" method="POST">
                <input type="text" name="id" placeholder="Member Id"><br>
                <input type="text" name="group" placeholder="Group name"><br>
                <input type="text" name="fname" placeholder="First Name"><br>
                <input type="text" name="lname" placeholder="Last Name"><br>
                <input type="email" name="email" placeholder="E-mail"><br>
                <input type="text" name="tel" placeholder="Mobile Number"><br>
                <input type="text" name="plot" placeholder="plot size in acres"><br>
                <input type="text" name="plot_nature" placeholder="Nature of plot"><br>
                <input type="text" name="valueChain" placeholder="member value chain"><br>
                <input type="text" name="investment" placeholder="Investment in ksh"><br>
                <input type="text" name="harvest" placeholder="Projected harvest duration in months"><br>
                <input type="text" name="weight" placeholder="Projected harvest in Kg"><br>
                <button>Register</button>
            </form>
        </div>
    </div>
</body>
</html>